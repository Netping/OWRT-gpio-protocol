config globals 'globals'
	list direction 'in.Вход'
	list direction 'out.Выход'
	list protocol 'D_triger_IO.Д-триггер-Ввод/Вывод'
	list protocol 'D_triger_Relay.Д-триггер-реле'
	list protocol 'Fake_IO.Имитация'

config device 'prototype'
	option name 'prototype'
	option protocol 'Fake-IO'
	option inputs '0'
	option outputs '0'
	option other '0'

config device
	option name 'usb'
	option protocol 'Fake-IO'
	option inputs '1'
	option outputs '1'
	option other '0'
